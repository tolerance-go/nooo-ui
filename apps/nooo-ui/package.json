{
   "name": "@fenxing/nooo-ui",
   "version": "1.0.0",
   "keywords": [],
   "license": "ISC",
   "author": "",
   "scripts": {
      "build": "next build",
      "clean": "rimraf .next node_modules .turbo .vscode",
      "postbuild": "pnpm collect",
      "collect": "concurrently --max-processes 1 \"npm:collect:*\"",
      "collect:designer-css": "cpy ./node_modules/@fenxing/nooo-ui-designer/dist-css/styles.css ./public/collect --rename nooo-ui-designer.css",
      "collect:designer-scripts": "cpy ./node_modules/@fenxing/nooo-ui-designer/dist-merged/browser-merged.global.js ./public/collect --rename nooo-ui-designer.js",
      "dev": "concurrently \"npm:serve:next\" \"npm:watch:*\"",
      "lint": "next lint",
      "open:browser": "open http://localhost:3000",
      "serve:next": "pnpm open:browser && next dev",
      "start": "next start -p 3001",
      "watch:collect:designer-css": "fenxing --listen-file-and-exec --listen-file ./node_modules/@fenxing/nooo-ui-designer/dist-css/styles.css --exec '`pnpm collect:designer-css`'",
      "watch:collect:dist-design": "fenxing --listen-file-and-exec --listen-file ./node_modules/@fenxing/nooo-ui-designer/dist-merged/browser-merged.global.js --exec '`pnpm collect:designer-scripts`'"
   },
   "dependencies": {
      "@fenxing/app-project-dashboard": "workspace:^1.0.0",
      "@fenxing/class-tags-input": "workspace:^1.0.0",
      "@fenxing/fenxing-loginer": "workspace:^1.0.0",
      "@fenxing/nooo-ui-common": "workspace:^1.0.0",
      "@fenxing/nooo-ui-db": "workspace:*",
      "@fenxing/nooo-ui-designer": "workspace:^1.0.0",
      "@fenxing/react-alert": "workspace:^1.0.0",
      "@fenxing/react-components": "workspace:^1.0.0",
      "@fenxing/request": "workspace:^1.0.0",
      "@fenxing/tailwind-class": "workspace:^1.0.0",
      "@fenxing/user-settings-dashboard": "workspace:^1.0.0",
      "@fenxing/utils": "workspace:^1.0.0",
      "animate.css": "^4.1.1",
      "axios": "^1.2.0",
      "clsx": "^1.2.1",
      "file-saver": "^2.0.5",
      "formidable": "^2.0.1",
      "iron-session": "workspace:^6.3.1",
      "lodash-es": "^4.17.21",
      "lodash.groupby": "^4.6.0",
      "next": "^13.0.7",
      "qs": "^6.11.0",
      "react": "^18.2.0",
      "react-dnd": "^16.0.1",
      "react-dnd-html5-backend": "^16.0.1",
      "react-dom": "^18.2.0",
      "swr": "^1.3.0",
      "tiny-invariant": "^1.3.1",
      "uid": "^2.0.1"
   },
   "devDependencies": {
      "@fenxing/scripts": "workspace:^1.0.0",
      "@tailwindcss/forms": "^0.5.3",
      "@tailwindcss/typography": "^0.5.8",
      "@types/file-saver": "^2.0.5",
      "@types/jquery": "^3.5.14",
      "@types/lodash-es": "^4.17.6",
      "@types/lodash.groupby": "^4.6.7",
      "@types/nprogress": "^0.2.0",
      "@types/qs": "^6.9.7",
      "@types/react": "^18.0.25",
      "autoprefixer": "^10.4.13",
      "concurrently": "^7.6.0",
      "cpy-cli": "workspace:^4.2.0",
      "eslint-config-next": "^13.0.5",
      "postcss": "^8.4.19",
      "tailwindcss": "workspace:^3.2.4",
      "tsup": "workspace:^6.5.0",
      "tsx": "^3.12.1"
   }
}
